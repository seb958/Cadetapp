'Erreur', 'Impossible de sauvegarder le sous-groupe');\n      } finally {\n        setSavingSubGroup(false);\n      }\n    };\n    const deleteSubGroup = async subGroup => {\n      showConfirmation('Supprimer le sous-groupe', `Êtes-vous sûr de vouloir supprimer définitivement le sous-groupe \"${subGroup.nom}\" ?\\n\\nTous les cadets affectés à ce sous-groupe seront désaffectés.`, async () => {\n        try {\n          const token = await AsyncStorage.default.getItem('access_token');\n          const response = await fetch(`${EXPO_PUBLIC_BACKEND_URL}/api/subgroups/${subGroup.id}`, {\n            method: 'DELETE',\n            headers: {\n              'Authorization': `Bearer ${token}`\n            }\n          });\n          if (response.ok) {\n            showAlert('Succès', `Le sous-groupe \"${subGroup.nom}\" a été supprimé définitivement.`);\n            await loadSubGroups();\n            await loadUsers(); // Recharger les utilisateurs car leurs sous-groupes ont pu changer\n          } else {\n            const errorData = await response.json();\n            showAlert('Erreur', errorData.detail || 'Impossible de supprimer le sous-groupe');\n          }\n        } catch (error) {\n          console.error('Erreur lors de la suppression:', error);\n          showAlert('Erreur', 'Erreur réseau lors de la suppression');\n        }\n      });\n    };\n    const toggleSectionExpansion = sectionId => {\n      setExpandedSections(prev => {\n        const newExpanded = new Set(prev);\n        if (newExpanded.has(sectionId)) {\n          newExpanded.delete(sectionId);\n        } else {\n          newExpanded.add(sectionId);\n        }\n        return newExpanded;\n      });\n    };\n    const getSubGroupsForSection = sectionId => {\n      return subGroups.filter(sg => sg.section_id === sectionId);\n    };\n    const getUserCountForSubGroup = subGroupId => {\n      return users.filter(u => u.subgroup_id === subGroupId && u.actif).length;\n    };\n    const getUserCountForSection = sectionId => {\n      // Compter les utilisateurs directement dans la section + ceux dans les sous-groupes de cette section\n      const directUsers = users.filter(u => u.section_id === sectionId && !u.subgroup_id && u.actif).length;\n      const subGroupUsers = users.filter(u => {\n        const userSubGroup = subGroups.find(sg => sg.id === u.subgroup_id);\n        return userSubGroup && userSubGroup.section_id === sectionId && u.actif;\n      }).length;\n      return directUsers + subGroupUsers;\n    };\n    const getResponsableName = responsableId => {\n      const responsable = users.find(u => u.id === responsableId);\n      return responsable ? `${responsable.prenom} ${responsable.nom}` : 'Aucun responsable';\n    };\n    const toggleCadetSelection = cadetId => {\n      setActivityForm(prev => ({\n        ...prev,\n        cadet_ids: prev.cadet_ids.includes(cadetId) ? prev.cadet_ids.filter(id => id !== cadetId) : [...prev.cadet_ids, cadetId]\n      }));\n    };\n    if (loading) {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n        style: styles.container,\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: styles.centerContent,\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ActivityIndicator.default, {\n            size: \"large\",\n            color: \"#3182ce\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 1551,\n            columnNumber: 11\n          }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: styles.loadingText,\n            children: \"Chargement...\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 1552,\n            columnNumber: 11\n          }, this)]\n        }, void 0, true, {\n          fileName: _jsxFileName,\n          lineNumber: 1550,\n          columnNumber: 9\n        }, t